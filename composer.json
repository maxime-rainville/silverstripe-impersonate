{
    "name": "maxime-rainville/silverstripe-impersonate",
    "description": "Authenticator that allows you to impersonate any other user",
    "type": "silverstripe-vendormodule",
    "license": "MIT",
    "authors": [
        {
            "name": "Maxime Rainville",
            "email": "maxime@rainville.me"
        }
    ],
    "require": {
        "silverstripe/framework": "^4",
        "silverstripe/vendor-plugin": "^1.0"
    },
    "require-dev": {
        "phpunit/PHPUnit": "~4.8",
        "squizlabs/php_codesniffer": "^3"
    },
    "extra": {
        "expose": [
        ]
    },
    "autoload": {
        "psr-4": {
            "MaximeRainville\\Impersonate\\": "src/",
            "MaximeRainville\\Impersonate\\Tests\\": "tests/php/"
        }
    },
    "scripts": {
        "lint": "phpcs src/ tests/php/",
        "lint-clean": "phpcbf src/ tests/php/"
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
